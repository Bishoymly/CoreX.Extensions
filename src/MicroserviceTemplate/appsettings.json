{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=MicroserviceTemplate;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "FeatureManagement": {
    "HttpLogger": true,
    "Healthz": true,
    "HealthUI": false,
    "Swagger": true,
    "ForwardedHeaders": true,
    "Metrics": true,
    "DeveloperDashboard": true
  },
  "HttpLogger": {
    "Enabled": true,
    "TimestampFormat": "hh:mm:ss.fff",
    "AllowForAnonymous": true,
    "AllowForUser": "",
    "AllowForRole": ""
  },
  "HttpClientLogging": {
    "Enabled": true,
    "Headers": false,
    "Body":  false
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "MicroserviceTemplate",
        "Uri": "https://localhost:44383/healthz"
      }
    ],
    "EvaluationTimeOnSeconds": 30,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "AllowedHosts": "*"
}
